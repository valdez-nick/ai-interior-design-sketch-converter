# AI Service API Keys
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
GOOGLE_AI_API_KEY=your_google_ai_api_key_here
RUNPOD_API_KEY=your_runpod_api_key_here

# AI Service Endpoints
RUNPOD_ENDPOINT=https://api.runpod.ai/v2/your-endpoint-id/runsync
LOCAL_AI_ENDPOINT=http://localhost:7860

# Application Configuration
NODE_ENV=development
MAX_FILE_SIZE=10485760
MAX_BATCH_SIZE=10
RATE_LIMIT_PER_MINUTE=20

# Optional: Analytics and Monitoring
ANALYTICS_ID=your_analytics_id
SENTRY_DSN=your_sentry_dsn

# Security
ALLOWED_ORIGINS=https://your-domain.com,https://your-domain.vercel.app
JWT_SECRET=your_jwt_secret_for_session_management